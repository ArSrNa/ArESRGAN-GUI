{
  "name": "esrganui",
  "productName": "ArSrNaESRGANUI",
  "version": "2.5.0",
  "description": "Powered by Ar-Sr-Na",
  "main": "./src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "builder": "electron-builder --win --x64",
    "build": "electron-packager ./ ArSrNaESRGAN --win --ignore test/ --ignore realsgan/ --ignore electron-v17.0.0-win32-x64.zip --arch=x64 --out ./out --electron-zip-dir=./ --icon=./icon.ico --overwrite --asar"
  },
  "build": {
    "productName": "ArSrNaUIESRGAN",
    "appId": "arsrnauiesrgan",
    "copyright": "ArSrNaRenderInfinity",
    "asar": true,
    "directories": {
      "output": "build"
    },
    "win": {
      "icon": "./icon.ico",
      "target": [
        "nsis",
        "zip"
      ]
    },
    "asarUnpack": [],
    "extraResources": [
      {
        "from": "backres/",
        "to": "extraResources/"
      }
    ],
    "files": [
      "!electron-v17.0.0-win32-x64.zip",
      "!backres/"
    ],
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./icon.ico",
      "uninstallerIcon": "./icon.ico",
      "installerHeaderIcon": "./icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "ArESRGAN图像版"
    },
    "electronDownload": {
      "mirror": "https://npmmirror.com/mirrors/electron/"
    }
  },
  "keywords": [],
  "author": "结城明日奈",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "aresrgan"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "ciam-node-sdk": "^1.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.2",
    "express-ws": "^5.0.2",
    "node-cmd": "^5.0.0",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "electron": "17.0.0",
    "electron-packager": "^15.5.1",
    "semver": "^7.3.7"
  }
}